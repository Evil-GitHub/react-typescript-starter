# 环境变量示例文件
# 复制此文件为 .env.local 并填入实际值

# 应用基础配置
REACT_APP_ENV=dev
NODE_ENV=development
UMI_ENV=dev

# API 配置
REACT_APP_API_BASE_URL=http://192.168.100.102:32355
REACT_APP_REQUEST_BASE_URL=http://192.168.100.102:32355

# 应用配置
REACT_APP_TITLE=Evil Pro
REACT_APP_TOKEN_KEY=RKLINK_TOKEN
REACT_APP_LOGO_PATH=/images/logo.png

# 构建配置
REACT_APP_BUILD_VERSION=1.2.0
REACT_APP_BUILD_TIME=
